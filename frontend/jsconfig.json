{
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@context/*": [
        "src/context/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@/*": [
        "src/*"
      ],
      "@ProductDetailModal/*": [
        "src/components/ProductDetailModal/*"
      ],
      "@ChangePasswordModal/*": [
        "src/components/ChangePasswordModal/*"
      ]
    }
  }
}